<!doctype html>
<html lang="en">
  {% include 'dashboard/head.html.twig' %}
  <body>
		
		<div class="wrapper d-flex align-items-stretch position-fixed">
			{% include 'dashboard/side_navbar.html.twig' %}

        <!-- Page Content  -->
      <div id="content" class="p-4 p-md-5 scr scrollable_Y">

        <nav class="navbar navbar-expand-lg navbar-light bg-lightd">
          <div class="container-fluid">

            <button type="button" id="sidebarCollapse" class="btn btn-primary">
              <i class="fa fa-bars"></i>
              <span class="sr-only">Toggle Menu</span>
            </button>
            <button class="btn btn-dark d-inline-block d-lg-none ml-auto" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                <i class="fa fa-bars"></i>
            </button>
            

            <div class="collapse navbar-collapse" id="navbarSupportedContent">
              <ul class="nav navbar-nav ml-auto">
                <li class="nav-item active">
                    {% block search_bar %}{% endblock %}
                </li>
                
              </ul>
            </div>
          </div>
        </nav>


        
        <div>
          {% block section %}{% endblock %}
        </div>
        
      </div>
		</div>
  


    <script src="{{asset('new/js/jquery.min.js')}}"></script>
    <script src="{{asset('new/js/popper.js')}}"></script>
    <script src="{{asset('new/js/bootstrap.min.js')}}"></script>
    <script src="{{asset('new/js/main.js')}}"></script>
    {% block jscript %}{% endblock %}
  </body>
</html>